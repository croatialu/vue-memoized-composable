{
  "name": "vue-memoized-composable",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.4.1",
  "description": "A Vue composable that memoizes the result of a function",
  "author": "Croatia Lu <wuli.croatia@foxmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/croatialu",
  "homepage": "https://github.com/croatialu/vue-memoized-composable#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/croatialu/vue-memoized-composable.git"
  },
  "bugs": "https://github.com/croatialu/vue-memoized-composable/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "unbuild",
    "build:watch": "unbuild --watch",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@tanstack/vue-query": ">=5",
    "vue": ">=3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^8.1.0",
    "@tanstack/vue-query": "^5.66.4",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.2",
    "eslint": "^9.20.0",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.2.1",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "vue": "^3.5.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
